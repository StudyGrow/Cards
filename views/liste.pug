.container
    h1 Übersicht  
    #accordion
      - var n = 0;
      while n < karten.length
        .card
          #headingOne.card-header
            h5.mb-0
              button.btn.btn-link(data-toggle='collapse' data-target='#c'+karten[n]._id aria-expanded='true' aria-controls='collapseOne')
                | #{karten[n].thema}
          .collapse(id='c'+karten[n]._id aria-labelledby='headingOne' data-parent='#accordion')
            .card-body
              | #{karten[n].content}
        -n++
      